import java.util.*;
//Find the Ceil
//Ceil value of a real number X is the smallest integer value that is greater than or equal to X
class Main {
	public static int solve(int A) {
        double ans= A/200d;
        int x = (int)ans; // Type casting double to integer and storing the value int the x
        if(ans == x){
            return x;
        }
        else if(ans>=0){
            return x+1;
        }
        else{
            return x; 
        }
    }
	public static void main(String args[]) {
		// Your code goes here
		Scanner sc = new Scanner(System.in);
		int A = sc.nextInt();
		int answer = solve(A);
		System.out.println(answer);
		
}
}
